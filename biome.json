{
	"$schema": "https://biomejs.dev/schemas/1.6.4/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"javascript": {
		"formatter": {
			"quoteProperties":"asNeeded",
			"arrowParentheses":"asNeeded",
			"quoteStyle":"single",
			"indentStyle":"tab",
			"trailingComma":"none"
		}
	},
	"json": {
		"formatter": {
			"indentStyle":"tab"
	}
}
}
